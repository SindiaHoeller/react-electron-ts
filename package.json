{
  "name": "react-electron-ts",
  "version": "1.0.0",
  "description": "",
  "homepage": ".",
  "main": "build/electron/main.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.common.js",
    "electron:start": "set NODE_ENV=development && concurrently \"cross-env BROWSER=none webpack serve --config webpack.electron.js\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "webpack --config webpack.electron.js && tsc -p electron && electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/electron-devtools-installer": "^2.2.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.0",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "build": {
    "appId": "com.test.app",
    "productName": "ReactElectron",
    "copyright": "Copyright Â©Aki 2022",
    "buildVersion": "0.0.1",
    "compression": "maximum",
    "extends": null,
    "files": [
      "**",
      "./src/assets/**",
      "./public/icon.*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    },
    "extraFiles": [
      {
        "from": "public/assets",
        "to": "public/assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "include": "nsisExtra.nsh"
    },
    "win": {
      "icon": "./public/assets/icon.ico",
      "target": "NSIS"
    },
    "linux": {
      "icon": "./public/assets/icon_120x120.png"
    }
  }
}
